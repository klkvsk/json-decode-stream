{
    "name": "klkvsk/json-decode-stream",
    "description": "JSON streaming reader",
    "homepage": "https://github.com/klkvsk/json-decode-stream",
    "keywords": [
        "json",
        "parse",
        "decode",
        "stream"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mikhail Kulakovskiy",
            "email": "m@klkvsk.ru"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "JsonDecodeStream\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JsonDecodeStream\\Tests\\": "tests"
        }
    },
    "require": {
        "php": ">=7.1",
        "ext-json": "*"
    },
    "require-dev": {
        "psr/http-message": "^1.0",
        "nyholm/psr7": "^1.3",
        "phpunit/phpunit": "^7.0",
        "vimeo/psalm": "*"
    }
}
